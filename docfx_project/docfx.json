{
	"metadata": [
		{
			"src": [
				{
					"files": [
						"TCParticlesMain.Plugins.csproj"
					],

					"exclude": [
						"**/obj/**",
						"**/bin/**",
						"TCParticlesDocumentation/**"
					],

					"src": "../"
				}
			],

			"dest": "api",
			"filter": "filterConfig.yml"
		}
	],
	"build": {
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"articles/**.md",
					"articles/**/toc.yml",
					"toc.yml",
					"*.md"
				],
				"exclude": [
					"obj/**",
					"TCParticlesDocumentation/**"
				]
			}
		],
		"resource": [
			{
				"files": [
					"images/**"
				],
				"exclude": [
					"obj/**",
					"TCParticlesDocumentation/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md"
				],
				"exclude": [
					"obj/**",
					"TCParticlesDocumentation/**"
				]
			}
		],
		"dest": "TCParticlesDocumentation",
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"template": [
			"_exported_templates/default"
		],
		"postProcessors": [],
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false
	}
}